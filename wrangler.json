{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "cod-stats-file-parser",
	"main": "src/index.ts",
	"compatibility_date": "2025-01-09",
	"compatibility_flags": ["nodejs_compat"],
	"observability": {
		"enabled": true
	},
	"r2_buckets": [
		{
			"binding": "MY_BUCKET",
			"bucket_name": "cod-stats"
		}
	],
	"routes": [
		{
			"pattern": "api.cod-stats.jconet.ltd",
			"custom_domain": true
		}
	],
	"placement": {
		"mode": "smart"
	}
}
